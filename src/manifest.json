{
  "name": "Pivotal Markdown Link",
  "description": "Adds a button to pivotal story card to copy markdown link for Github",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://www.pivotaltracker.com/n/projects/*", "*://pivotaltracker.com/n/projects/*"],
      "css": ["pmdl.css"],
      "js": ["jquery.js", "pmdl.js"]
    }
  ]
}
